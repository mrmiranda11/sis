    <replace_variables>Y</replace_variables>
    <sql>declare
  lv_curr_table varchar2(28) := &apos;${curr_table_short}&apos;;
begin
  -- dbg(&apos;recreate aux tables.&apos;);
  
  begin
  	execute immediate &apos;drop table t_&apos; || lv_curr_table ;
  exception when others then null;
  end;

  execute immediate &apos;create table t_&apos; || lv_curr_table || &apos; as select 0 id, t.* from ${current_table} t where 1=0&apos;;
  dbg(&apos;t_&apos; || lv_curr_table ||&apos; table created&apos;);

  begin
  	execute immediate &apos;drop sequence s_&apos; || lv_curr_table ;
  exception when others then null;
  end;

  execute immediate &apos;create sequence s_&apos; || lv_curr_table ;  

  
  execute immediate &apos;
CREATE OR REPLACE TRIGGER g_&apos; || lv_curr_table || &apos;
  BEFORE INSERT 
  ON t_&apos; || lv_curr_table || &apos;
  FOR EACH ROW
DECLARE
  lv_id number;
BEGIN
  SELECT s_&apos; || lv_curr_table || &apos;.nextval INTO lv_id FROM DUAL;
  :new.id := lv_id;
END ;
  &apos;;


  begin
  	execute immediate &apos;drop table e_&apos; || lv_curr_table ;
  exception when others then null;
  end;
 
  DBMS_ERRLOG.CREATE_ERROR_LOG (
   dml_table_name            =&gt; &apos;${current_table}&apos;
  ,err_log_table_name        =&gt; &apos;e_&apos; || lv_curr_table 
  --,err_log_table_owner       IN VARCHAR2 := NULL,
  --,err_log_table_space       IN VARCHAR2 := NULL,
  --,skip_unsupported          IN BOOLEAN := FALSE
  );


exception when others then
  dbg(&apos;error: &apos; || SQLCODE || SUBSTR(SQLERRM, 1, 3000));
  raise;



end;</sql>
