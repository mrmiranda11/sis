    <sql>DECLARE
   @lv_str     varchar(4000)
  ,@lv_s       varchar(4000)
  ,@lv_t       varchar(4000)
  ,@lv_tt      varchar(4000)
  ,@lv_u       varchar(4000)
  ,@lv_pk_cols varchar(4000)
  ,@lv_pk_st   varchar(4000)
  ,@lv_dlm 	   varchar(3)
  ,@lv_sql     varchar(8000)

  ,@lv_fk_cols varchar(4000)
  ,@lv_fk_str  varchar(4000)
  , @fk_col_name varchar(100) 
  , @pk_tab_name varchar(100)
  , @pk_col_name varchar(100)
  , @pk_tab_name_old varchar(100)
  , @lv_dbg     varchar(8000)
begin

exec dbo.dbg @p_msg = &apos;starting...&apos;

  begin try
	execute(&apos;drop function dbo.rpl_str&apos;);
  end try
  begin catch 
    
  end catch	
execute(&apos;
create FUNCTION dbo.rpl_str(
    @p_str  varchar(8000)
  , @p_mask varchar(8000)
  , @p_dlm  varchar(100) 
)
RETURNS varchar(8000) 
AS 
BEGIN
    DECLARE 
        @pos_old  int
      , @pos_new  int
      , @lv_str   varchar(8000)
      , @lv_X     varchar(8000)
      , @rpl      varchar(8000)
      
    SELECT @pos_old = 1  
    SELECT @lv_str = &apos;&apos;&apos;&apos;  
    
    WHILE (1=1)
	BEGIN
		SELECT @pos_new = CHARINDEX(@p_dlm, @p_str + @p_dlm, @pos_old)     
        SELECT @lv_X = SUBSTRING(@p_str + @p_dlm, @pos_old, @pos_new - @pos_old)
        SELECT @pos_old = @pos_new + 1
        SELECT @rpl = replace(@p_mask, &apos;&apos;&lt;&lt;&lt;X&gt;&gt;&gt;&apos;&apos;, @lv_X)
        SELECT @lv_str = @lv_str + @rpl  
        IF (@pos_old &gt; DATALENGTH(@p_str)) BREAK
    END
	SELECT @lv_str = SUBSTRING(@lv_str, CHARINDEX(&apos;&apos; &apos;&apos;, @lv_str) + 1, DATALENGTH(@lv_str));
    	
    RETURN @lv_str;
END
&apos;)

set @lv_str = &apos;?&apos;; -- &apos;a,b,c&apos;
set @lv_dlm = &apos;${delimiter}&apos;;
exec dbo.dbg @p_msg = @lv_str
exec dbo.dbg @p_msg = @lv_dlm
------------------------------------------------------------
-- remove non-existing DB columns
declare
   @pos_old    int
  ,@pos_new    int
  ,@lv_X       varchar(4000)
  ,@lv_new_str varchar(4000)
  ,@lv_cnt     int
begin
    SELECT @pos_old = 1
	SET @lv_new_str = ''
    WHILE (1=1)        
    BEGIN                                                 
        SELECT @pos_new = CHARINDEX(@lv_dlm, @lv_str + @lv_dlm, @pos_old)
        SELECT @lv_X = SUBSTRING(@lv_str + @lv_dlm, @pos_old, @pos_new - @pos_old)
        SELECT @pos_old = @pos_new + 1
      
        select @lv_cnt = count(*)
          from INFORMATION_SCHEMA.columns c
         where c.table_name = &apos;${current_table}&apos;
           and c.column_name = @lv_X     
        
        if (@lv_cnt &gt; 0) 
            set @lv_new_str = @lv_new_str + &apos;,&apos; + @lv_X
        
        
	    IF (@pos_old &gt; DATALENGTH(@lv_str)) BREAK
    END

    set @lv_str = SUBSTRING(@lv_new_str, 2, 8000)          -- &apos;a,c&apos;
end;

----------------------------------




set @lv_tt = @lv_str;

set @lv_s = SUBSTRING(replace(&apos;,&apos; + @lv_str, &apos;,&apos;, &apos;,s.&apos;), 2, 8000) -- s.a,s.c
set @lv_t = SUBSTRING(replace(&apos;,&apos; + @lv_str, &apos;,&apos;, &apos;,t.&apos;), 2, 8000) -- t.a,t.c

set @lv_str = &apos;,&apos; + @lv_str + &apos;,&apos;

exec dbo.dbg @p_msg = @lv_s
exec dbo.dbg @p_msg = @lv_t

DECLARE @col_name varchar(100)

DECLARE c cursor local fast_forward
for
select cc.COLUMN_NAME 
  from INFORMATION_SCHEMA.table_constraints c
     , information_schema.KEY_COLUMN_USAGE  cc
 where c.table_name = &apos;${current_table}&apos;
   and c.constraint_type = &apos;PRIMARY KEY&apos;
   and cc.TABLE_CATALOG = c.TABLE_CATALOG
   and cc.TABLE_SCHEMA = c.TABLE_SCHEMA
   and cc.TABLE_NAME = c.TABLE_NAME
   and cc.CONSTRAINT_CATALOG = c.CONSTRAINT_CATALOG
   and cc.CONSTRAINT_SCHEMA = c.CONSTRAINT_SCHEMA
   and cc.CONSTRAINT_NAME = c.CONSTRAINT_NAME

open c

set @lv_pk_cols = &apos;&apos;

fetch next from c into @col_name
while (@@FETCH_STATUS = 0)
begin
	set @lv_pk_cols = @lv_pk_cols + &apos;,&apos; + @col_name
    set @lv_str = replace(@lv_str, &apos;,&apos; + @col_name + &apos;,&apos;, &apos;,&apos;)
    FETCH NEXT FROM c into @col_name
end 
close c
deallocate c

set @lv_pk_cols = substring(@lv_pk_cols, 2, 8000)

set @lv_dbg = &apos;@lv_pk_cols=&apos; + @lv_pk_cols
exec dbo.dbg @p_msg = @lv_dbg


if DATALENGTH(@lv_str) &gt; 1 
	begin
	  set @lv_str = substring(@lv_str, 2, DATALENGTH(@lv_str) - 2)
	  set @lv_u   = &apos;when matched then update set &apos; + dbo.rpl_str(@lv_str, &apos;, t.&lt;&lt;&lt;X&gt;&gt;&gt; = s.&lt;&lt;&lt;X&gt;&gt;&gt; &apos;, &apos;,&apos;)
	end  
else
	set @lv_u = &apos;&apos;


set @lv_pk_st = dbo.rpl_str(@lv_pk_cols, &apos;and t.&lt;&lt;&lt;X&gt;&gt;&gt; = s.&lt;&lt;&lt;X&gt;&gt;&gt; &apos;, &apos;,&apos;)


DECLARE cc cursor local fast_forward
for
SELECT  
     --KCU1.CONSTRAINT_NAME AS FK_CONSTRAINT_NAME 
    --,KCU1.TABLE_NAME AS FK_TABLE_NAME 
    KCU1.COLUMN_NAME AS FK_COLUMN_NAME 
    --,KCU1.ORDINAL_POSITION AS FK_ORDINAL_POSITION 
    --,KCU2.CONSTRAINT_NAME AS REFERENCED_CONSTRAINT_NAME 
    ,KCU2.TABLE_NAME AS REFERENCED_TABLE_NAME 
    ,KCU2.COLUMN_NAME AS REFERENCED_COLUMN_NAME 
    --,KCU2.ORDINAL_POSITION AS REFERENCED_ORDINAL_POSITION 
FROM INFORMATION_SCHEMA.REFERENTIAL_CONSTRAINTS AS RC 

INNER JOIN INFORMATION_SCHEMA.KEY_COLUMN_USAGE AS KCU1 
    ON KCU1.CONSTRAINT_CATALOG = RC.CONSTRAINT_CATALOG  
    AND KCU1.CONSTRAINT_SCHEMA = RC.CONSTRAINT_SCHEMA 
    AND KCU1.CONSTRAINT_NAME = RC.CONSTRAINT_NAME 
   and KCU1.TABLE_NAME = &apos;${current_table}&apos;
INNER JOIN INFORMATION_SCHEMA.KEY_COLUMN_USAGE AS KCU2 
    ON KCU2.CONSTRAINT_CATALOG = RC.UNIQUE_CONSTRAINT_CATALOG  
    AND KCU2.CONSTRAINT_SCHEMA = RC.UNIQUE_CONSTRAINT_SCHEMA 
    AND KCU2.CONSTRAINT_NAME = RC.UNIQUE_CONSTRAINT_NAME 
    AND KCU2.ORDINAL_POSITION = KCU1.ORDINAL_POSITION 
ORDER BY KCU2.TABLE_NAME

open cc

set @lv_fk_cols = &apos;&apos;
set @lv_fk_str = &apos;&apos;
set @pk_tab_name_old = &apos;&apos;


fetch next from cc into @fk_col_name, @pk_tab_name, @pk_col_name
while (@@FETCH_STATUS = 0)
begin
	set @lv_fk_cols = @lv_fk_cols + &apos;and t.&apos; + @pk_col_name + &apos; = s.&apos; + @fk_col_name + &apos; &apos;;
    
    if (@pk_tab_name &lt;&gt; @pk_tab_name_old) and (@pk_tab_name_old &lt;&gt; &apos;&apos;)  
    begin
		set @lv_fk_str = @lv_fk_str + &apos; and exists (select null from &apos; + @pk_tab_name_old + &apos; t where &apos; + substring(@lv_fk_cols, 4, 8000) + &apos;) &apos;

		set @lv_fk_cols = &apos;&apos;
        set @pk_tab_name_old = @pk_tab_name
 	end

    fetch next from cc into @fk_col_name, @pk_tab_name, @pk_col_name
end 

if @lv_fk_cols &lt;&gt; &apos;&apos; 
	set @lv_fk_str = @lv_fk_str + &apos; and exists (select null from &apos; + @pk_tab_name + &apos; t where &apos; + substring(@lv_fk_cols, 4, 8000) + &apos;) &apos;
else 
	set @lv_fk_str = &apos;&apos;

close cc
deallocate cc

set @lv_dbg = &apos;@lv_fk_str=&apos; + @lv_fk_str
exec dbo.dbg @p_msg = @lv_dbg


set @lv_sql = 
&apos;merge into ${current_table} t 
 using (
         select &apos; + @lv_s + &apos;
           from (
                  select s.*
                       , row_number() over (partition by &apos; + @lv_pk_cols + &apos; order by id desc ) rn
                    from t_${current_table} s
                   where application_id is not null  
                     &apos; + @lv_fk_str + &apos;
                 ) s
          where rn = 1    
      )   s  
       on ( &apos; + @lv_pk_st + &apos; ) 
  &apos;  + @lv_u + &apos; &apos;;

set @lv_sql = @lv_sql + &apos;
when not matched then insert (&apos; + @lv_tt + &apos;) values (&apos; + @lv_s + &apos;) ;
&apos;;

exec dbo.dbg @p_msg = @lv_sql


execute(@lv_sql);


exec dbo.dbg @p_msg = &apos;done!&apos;


&#47;*
execute(&apos;begin 
begin tran 
begin try 
&apos; + @lv_sql + &apos;
end try

begin catch
    SELECT 
        ERROR_SEVERITY() AS ErrorSeverity
        ,ERROR_STATE() AS ErrorState
        ,ERROR_MESSAGE() AS ErrorMessage;

    IF @@TRANCOUNT &gt; 0
        ROLLBACK TRANSACTION;
end catch;

IF @@TRANCOUNT &gt; 0
    COMMIT TRAN;

 end &apos;);
*&#47;



execute(&apos;drop table t_${current_table}&apos;);


end;


</sql>
