    <sql>declare
  lv_str     varchar2(4000);
  lv_s       varchar2(4000);
  lv_t       varchar2(4000);
  lv_u       varchar2(4000);
  lv_pk_cols varchar2(4000);
  lv_pk_st   varchar2(4000);

  lv_sql        clob;

  lv_curr_table varchar2(28) := &apos;${curr_table_short}&apos;;
  lv_dlm        varchar2(1)  := &apos;${delimiter}&apos;;


function rpl_str (
    p_str  varchar2
  , p_mask varchar2
  , p_dlm  varchar2 default &apos;,&apos;
)
return varchar2
is
    pos_old  number;
    pos_new  number;
    lv_str   varchar2(20000);
    lv_X     varchar2(20000);
begin
    pos_old := 1;                       
    loop                                                         
      pos_new := instr(p_str || p_dlm, p_dlm, pos_old);          
      lv_X    := substr(p_str || p_dlm, pos_old, pos_new - pos_old);
      pos_old := pos_new + 1;
      
      lv_str := lv_str || replace(p_mask, &apos;&lt;&lt;&lt;X&gt;&gt;&gt;&apos;, lv_X) ;     
      
      exit when pos_old &gt; length(p_str);
    end loop;
    lv_str := substr(lv_str, instr(lv_str, &apos; &apos;) + 1);
    return lv_str;
end;
  
begin

dbg(&apos;starting...&apos;);

lv_str := &apos;?&apos;; -- &apos;a|b|c&apos;

------------------
-- remove non-existing DB columns    
-- replace the delimiter with commas 
--&gt; &apos;a|b|c&apos; -&gt; &apos;a,c&apos;
declare
  pos_old    number;
  pos_new    number;
  lv_X       varchar2(4000);
  lv_new_str varchar2(4000);
  lv_cnt     number;
begin
    pos_old := 1;                       
    loop                                                         
        pos_new := instr( lv_str || lv_dlm, lv_dlm, pos_old);          
        lv_X    := substr(lv_str || lv_dlm, pos_old, pos_new - pos_old);
        pos_old := pos_new + 1;
      
        select count(*)
          into lv_cnt
          from all_tab_columns c
         where c.owner = USER --&gt;&gt; to be fixed later (second passed paramter is not working:()
           and c.table_name = &apos;${current_table}&apos;
           and c.column_name = upper(lv_X);     
        
        if lv_cnt &gt; 0 then
            lv_new_str := lv_new_str || &apos;,&apos; || lv_X; 
        end if;
        
        exit when pos_old &gt; length(lv_str);
    end loop;
    lv_str := substr(lv_new_str, 2);          -- &apos;a,c&apos;
end;
------------------

lv_s := substr(replace(&apos;,&apos; || lv_str, &apos;,&apos;, &apos;,s.&apos;), 2); -- s.a,s.c
lv_t := substr(replace(&apos;,&apos; || lv_str, &apos;,&apos;, &apos;,t.&apos;), 2); -- t.a,t.c

lv_str := &apos;,&apos; || lv_str || &apos;,&apos;;
for c in (
select column_name
  from all_constraints c
     , all_cons_columns cc
 where c.owner = USER --&gt;&gt; to be fixed later (second passed paramter is not working:()
   and c.table_name = &apos;${current_table}&apos;
   and c.constraint_type = &apos;P&apos;
   and cc.owner = c.owner
   and cc.constraint_name = c.constraint_name
   and cc.table_name = c.table_name
)
loop
	lv_pk_cols := lv_pk_cols || &apos;,&apos; || c.column_name;
    lv_str := replace(lv_str, &apos;,&apos; || c.column_name || &apos;,&apos;, &apos;,&apos;);
end loop;
lv_pk_cols := substr(lv_pk_cols, 2);

if length(lv_str) &gt; 1 then
  lv_str := substr(lv_str, 2, length(lv_str) - 2);
  lv_u   := &apos;when matched then update set &apos; || rpl_str(lv_str,   &apos;, t.&lt;&lt;&lt;X&gt;&gt;&gt; = s.&lt;&lt;&lt;X&gt;&gt;&gt; &apos;, &apos;,&apos;);
else
  lv_u := null;
end if;


lv_pk_st := rpl_str(lv_pk_cols, &apos;and t.&lt;&lt;&lt;X&gt;&gt;&gt; = s.&lt;&lt;&lt;X&gt;&gt;&gt; &apos;, &apos;,&apos;);

dbg(&apos;lv_pk_st=&apos; || lv_pk_st);


lv_sql := 
&apos;merge into ${current_table} t 
 using (
         select &apos; || lv_s || &apos; &apos;;



lv_sql := lv_sql || &apos;
           from (
                  select s.*
                       , row_number() over (partition by &apos; || lv_pk_cols || &apos; order by id desc  ) rn
                    from t_&apos; || lv_curr_table || &apos; s
                   --&gt;&gt;&gt;&gt;&gt; where application_id is not null   
                 ) s
          where rn = 1    
      )   s  
       on ( &apos; || lv_pk_st || &apos; ) 
  &apos;  || lv_u || &apos; &apos;;

lv_sql := lv_sql || &apos;
when not matched then insert (&apos; || lv_t || &apos;) values (&apos; || lv_s || &apos;) 
&apos;;

lv_sql := lv_sql || &apos;
 log errors into e_&apos; || lv_curr_table || &apos; (&apos;&apos;MERGE&apos;&apos;) reject limit unlimited &apos;;


dbg(lv_sql);
execute immediate lv_sql;
dbg(&apos;done!&apos;);

--execute immediate &apos;drop table t_&apos; || lv_curr_table || &apos;&apos;;

end;</sql>
