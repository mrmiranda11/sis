<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<process name="getJSONSchema" key="getJSONSchema" xsi:schemaLocation="http://jbpm.org/4.0/jpdl ../conf/jpdl-4.0.xsd" xmlns="http://jbpm.org/4.0/jpdl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <start name="Start">
        <transition to="Receive_HTTP"/>
    </start>
    <custom expr="#{preBeanActivity}" name="Receive_HTTP">
        <transition to="GenerateJSONSchema" name="success"/>
    </custom>
    <custom expr="#{groovyScriptActivity_GenerateJSONSchema}" name="GenerateJSONSchema">
        <transition to="End" name="success"/>
        <transition to="errorBundleHandler" name="error"/>
    </custom>
    <state name="End"/>
    <custom expr="#{errorBundleHandler}" name="errorBundleHandler">
        <transition to="End"/>
        <property name="params">
            <string value="errorMessage_key=error.message| destinationErrorCode_key=error.destination| errorCode_key=error.code| cpfId=getWSDL| errorParam_key=error.param"/>
        </property>
    </custom>
</process>
